{
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  // for more information regarding the rules please look at
  // http://eslint.org/docs/rules/
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 1,
    "no-case-declarations": 0,
    "no-unused-vars": ["error", {"args": "none"}]
  }
}